    "url": "git+https://github.com/mendersoftware/gui.git"
  },
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "sh ${MENDER_TESTING}/check_commits.sh"
    }
  },
  "scripts": {
    "build": "rspack --mode production",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx && eslint tests --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix && eslint tests --ext .js,.jsx,.ts,.tsx --fix",
    "serve": "./entrypoint.sh && rspack serve --mode development",
    "test": "vitest --dir src/js",
    "test-types": "tsc --project ./tsconfig-test.json",
    "watch": "rspack --watch --mode development"
  },
